{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Restie",
  "version": "0.1.0",
  "identifier": "com.mise42.restie",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
